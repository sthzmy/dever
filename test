import  java.util.Scanner;

public class media{

    public static void main(String [ ] args){

        Scanner entrada = new Scanner(System.in);
        int contador=0;
        while (contador<10){
            System.out.print("contagem de 0 a 10");
            String nome= 0;
            System.out.println(nome);
            contador= contador+1;
        }//while
    }//main
}//class

